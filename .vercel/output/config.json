{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "index.astro"
		},
		{
			"src": "^\\/rss\\.xml$",
			"dest": "rss.xml.js"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "about.astro"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "blog/index.astro"
		},
		{
			"src": "^\\/blog(?:\\/(.*?))?\\/?$",
			"dest": "blog/[...slug].astro"
		}
	]
}